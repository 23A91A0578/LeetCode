class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        Set<Integer> ht=new HashSet<>();
        
        for(int i=0;i<nums1.length;i++){
            for(int j=0;j<nums2.length;j++){
                if(nums1[i]==nums2[j]){
                    ht.add(nums1[i]);
                    break;
                }
            }
        }
        int[] res=new int[ht.size()];
        int hey=0;
        for(Integer k:ht){
            res[hey]=k;
            hey=hey+1;
        }
        return res;
    }
}
