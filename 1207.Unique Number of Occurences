class Solution {
    public boolean uniqueOccurrences(int[] arr) {
        HashMap<Integer,Integer> freq=new HashMap<>();
        for(int i=0;i<arr.length;i++){
            freq.put(arr[i],freq.getOrDefault(arr[i],0)+1);
        }
        HashSet<Integer> s=new HashSet<>();
        for(int i:freq.values()){
            if(!s.add(i)){
                return false;
            }
        }
        return true;
    }
}
