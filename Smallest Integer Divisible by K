class Solution {
    public int smallestRepunitDivByK(int k) {
        if(k%2==0 || k%5==0){
            return -1;
        }
        if(k==1){
            return 1;
        }
        int rem=1;
        int c=1;
        while(rem!=0){
            rem=(rem*10+1)%k;
            c=c+1;
        }
        return c;
    }
}
