class Solution {
    public int romanToInt(String s) {
        int res=0;
        Map<Character,Integer> rom=new HashMap<>();
        rom.put('I',1);
        rom.put('V',5);
        rom.put('X',10);
        rom.put('L',50);
        rom.put('C',100);
        rom.put('D',500);
        rom.put('M',1000);
        int i=0;
        while(i<s.length()-1){
            if(rom.get(s.charAt(i))<rom.get(s.charAt(i+1))){
                res=res+rom.get(s.charAt(i+1))-rom.get(s.charAt(i));
                i=i+2;
            }
            else{
                res=res+rom.get(s.charAt(i));
                i=i+1;
            }
        }
        if(i!=s.length()){
            res=res+rom.get(s.charAt(s.length()-1));
        }
        return res;
    }
}
