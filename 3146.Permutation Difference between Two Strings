class Solution {
    public int findPermutationDifference(String s, String t) {
        int permut_diff = 0;
        for (int i = 0; i < s.length(); i++) {
            for (int j = 0; j < t.length(); j++) {
                if (s.charAt(i) == t.charAt(j)) {
                    permut_diff = permut_diff + Math.abs(i - j);
                    break;
                }
            }
        }
        return permut_diff;
    }
}
