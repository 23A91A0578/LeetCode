class Solution {
    public long zeroFilledSubarray(int[] nums) {
        long ans=0;
        long cur=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==0){
                cur++;
                ans=ans+cur;
            }
            else{
                cur=0;
            }
        }
        return ans;
    }
}
