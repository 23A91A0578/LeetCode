class Solution {
    public int triangularSum(int[] nums) {
        int n=nums.length;
        while(n!=1){
            int[] newnums=new int[n-1];
            for(int i=0;i<newnums.length;i++){
                newnums[i]=(nums[i]+nums[i+1])%10;
            }
            n--;
            for(int j=0;j<n;j++){
                nums[j]=newnums[j];
            }
        }
        return nums[0];
    }
}
