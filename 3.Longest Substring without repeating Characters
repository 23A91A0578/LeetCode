class Solution {
    public int lengthOfLongestSubstring(String s) {
        int n=s.length();
        int l=0;
        HashSet<Character> hs=new HashSet<>();
        int maxlen=0;
        for(int r=0;r<n;r++){
            while(hs.contains(s.charAt(r))){
                hs.remove(s.charAt(l));
                l++;
            }
            hs.add(s.charAt(r));
            maxlen=Math.max(hs.size(),maxlen);
        }
        return maxlen;
    }
}
