class Solution {
    public int minNumberOperations(int[] target) {
        int[] initial=new int[target.length];
        initial[0]=target[0];
        for(int i=1;i<target.length;i++){
            if(target[i]-target[i-1]>0){
                initial[i]=target[i]-target[i-1];
            }
        }
        int s=0;
        for(int i=0;i<initial.length;i++){
            if(initial[i]>0){
                s=s+initial[i];
            }
        }
        return s;
    }
}
