class Solution {
    public List<Integer> targetIndices(int[] nums, int target) {
        ArrayList<Integer> l=new ArrayList<>();
        int sc=0;
        int tc=0;
        for(int num:nums){
            if(num<target){
                sc++;
            }
            else if(num==target){
                tc++;
            }
        }
        for(int i=sc;i<sc+tc;i++){
            l.add(i);
        }
        return l;
    }
}
