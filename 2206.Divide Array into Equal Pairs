class Solution {
    public int sumOfUnique(int[] nums) {
        int ans = 0;
        HashMap<Integer,Integer> map = new HashMap<>();

        for(int num : nums){
            map.put(num,map.getOrDefault(num,0)+1);
            if(map.get(num)==1){
                ans += num;
            } else if(map.get(num) == 2){
                ans -= num;
            }
        }
        return ans;
    }
}
