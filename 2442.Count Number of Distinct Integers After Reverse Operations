class Solution {
    public int rev(int n){
        int rev=0;
        int r=0;
        while(n>0){
            r=n%10;
            rev=rev*10+r;
            n=n/10;
        }
        return rev;
    }
    public int countDistinctIntegers(int[] nums) {
        Set<Integer> res=new HashSet<>();
        for(int i=0;i<nums.length;i++){
            res.add(nums[i]);
            res.add(rev(nums[i]));
        }
        return res.size();
    }
}
